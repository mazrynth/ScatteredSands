
var Player : Transform;
 var MaxDist = 5;
 var MinDist = 2;
 
 
 
 
 function Start () 
 {
 	print("Working2");
 }
 
 function Update () 
 {      
	  if(Vector3.Distance(transform.position,Player.position) <= MaxDist)
	    {
	    	
	         print("Player Wins!");
	         //Returns player to main menu:
             SceneManager.LoadScene ("Start_Menu");
		} 
    

 }
